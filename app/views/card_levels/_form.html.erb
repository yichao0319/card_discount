<%= form_for @card_level do |f| %>

    <% if @card_level.errors.any? %>
        <div id="error_explanation">
            <h2>
                <%= pluralize(@card_level.errors.count, "error") %> prohibited
                this card_level from being saved:
            </h2>
            <ul>
                <% @card_level.errors.full_messages.each do |msg| %>
                <li><%= msg %></li>
                <% end %>
            </ul>
        </div>
    <% end %>

    <p>
        <%= f.label :Level %><br>
        <%= f.text_field :level %>
    </p>

    <p>
        <%= f.label :name %><br>
        <%= f.text_field :name %>
    </p>

    <p>
        <%= f.label :機構 %><br>
        <%= f.collection_select :agent_id, Agent.order(:name),:id,:name, include_blank: true %>
    </p>

    <p>
        <%= f.submit %>
    </p>

<% end %>